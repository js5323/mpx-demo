    "webpack-merge": "^4.1.2"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js --fix"
    ],
    "src/*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ],
    "src/*.{js,mpx}": "eslint --cache --fix"
  }
}
