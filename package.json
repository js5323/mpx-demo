{
  "name": "mpx-project",
  "version": "1.0.0",
  "description": "A mpx project",
  "main": "index.js",
  "scripts": {
    "watch": "node ./build/build.js -w",
    "watch:prod": "node ./build/build.js -w -p",
    "build": "node ./build/build.js -p",
    "build:dev": "node ./build/build.js",
    "watch:cross": "npm run watch --wx --ali --web",
    "build:cross": "npm run build --wx --ali --web",
    "watch:web:serve": "npx npm-run-all --parallel watch:web serve",
    "watch:web": "npm run watch --web",
    "serve": "npx http-server dist/web",
    "lint": "eslint --ext .js,.mpx src/",
    "help": "node ./build/build.js --help",
    "prepare": "husky install"
  },
  "author": "js5323 <js5323@hotmail.com>",
  "license": "ISC",
  "dependencies": {
    "@mpxjs/api-proxy": "^2.5.10",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.3",
    "@mpxjs/core": "^2.6.31",
    "@vant/weapp": "^1.6.8"
  },
  "browserslist": [
    "ios >= 9",
    "chrome >= 47"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime-corejs3": "^7.10.4",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@mpxjs/webpack-plugin": "^2.6.31",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "chalk": "^2.3.2",
    "commander": "^6.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^0.28.11",
    "eslint": "^6.0.0",
    "eslint-config-babel": "^8.0.2",
    "eslint-config-prettier": "^8.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^6.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-local-rules": "^0.1.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
